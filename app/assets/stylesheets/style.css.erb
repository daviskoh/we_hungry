.nav {
  margin-bottom: 4em;
  background: #D60009;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-height: 9%;
  border-radius: 0px 0px 10px 10px;
  -moz-box-shadow: 1px 1px 3px 2px #BABAC4;
  -webkit-box-shadow: 1px 1px 5px 3px #BABAC4;
  box-shadow: 1px 1px 3px 3px #BABAC4;
  opacity: .8;
  z-index: 2;
}
nav ul li {
  display: inline-block;
}

.nav_button {
  text-decoration: none;
  /*color: #36BEE6;*/
  color: #000000;
  border: 2px solid rgb(85%,85%,85%);
  border-radius: 10px;
  padding: 5px;
}
.nav_button:hover {
  color: #01FDFD;
  background-color: #F31749;
}
.nav_button:active {
  border-style: inset;
}

